; Conditional Branch Instruction Function Decleartions.
; This function do not change. Very dangerous
; 7, July, 1992 Insertions.

PREAMBLE

; *** Branch near relative address if accumulator is zero ***

bzo	function	r8
	local	_next_
	bnz	_next_
	bro	r8
_next_:
	endf

; *** Branch near relative address if accumulator is not zero ***

bnzo	function	r8
	local	_next_
	bz	_next_
	bro	r8
_next_:
	endf

; *** Branch near relative address if direct bit is positive ***

bpo	function	d9,b3,r8
	local	_next_
	local	_true_
	bp	d9,b3,_true_
	br	_next_
_true_:	bro	r8
_next_:
	endf

; *** Branch near relative address if direct bit is positive, and clear ***

bpco	function	d9,b3,r8
	local	_next_
	local	_true_
	bpc	d9,b3,_true_
	br	_next_
_true_:	bro	r8
_next_:
	endf

; *** Branch near relative address if direct bit is negative ***

bno	function	d9,b3,r8
	local	_next_
	local	_true_
	bn	d9,b3,_true_
	br	_next_
_true_:	bro	r8
_next_:
	endf

; *** Decrement direct byte and branch near relative address if direct byte is not zero ***

dbnzo	function	d9,r8
	local	_next_
	local	_true_
	dbnz	d9,_true_
	br	_next_
_true_:	bro	r8
_next_:
	endf

; *** Compare immediate data or accumulator and branch near relative address if equal ***

beo	function	arg0,arg1,arg2
	local	_next_
	local	_txen_
	ifb	<<arg2>>
	bne	arg0,_next_
	bro	arg1
_next_:
	else
	bne	arg0,arg1,_txen_
