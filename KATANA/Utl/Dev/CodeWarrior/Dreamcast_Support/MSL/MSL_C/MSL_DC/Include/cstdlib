#ifndef	_CSTDLIB_
#define _CSTDLIB_

#include <stdlib.h>

#ifdef __cplusplus
	#ifdef _MSL_USING_NAMESPACE
		namespace std {
	#endif
	extern "C" {
#endif

void				abort(void);
int					atexit(void (*func)(void));
void				exit(int status);
unsigned long		strtoul(const char * str, char ** end, int base);

#ifdef __cplusplus
	}
	#ifdef _MSL_USING_NAMESPACE
		}
	#endif
#endif

#endif /* _CSTDLIB_  */

