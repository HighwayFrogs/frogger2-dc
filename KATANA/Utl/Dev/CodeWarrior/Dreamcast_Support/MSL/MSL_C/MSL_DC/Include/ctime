/*
 *	ctime
 */


#ifndef _CTIME_
#define _CTIME_

#include <time.h>

#ifdef __cplusplus
	#ifdef _MSL_USING_NAMESPACE
		namespace std {
	#endif
	extern "C" {
#endif

struct tm {
	int	tm_sec;
	int	tm_min;
	int	tm_hour;
	int	tm_mday;
	int	tm_mon;
	int	tm_year;
	int	tm_wday;
	int	tm_yday;
	int	tm_isdst;
};

#ifdef __cplusplus
	}
	#ifdef _MSL_USING_NAMESPACE
		}
	#endif
#endif

#endif /* _CTIME_  */

/*  Changes
 *  ad   990222	Include the O/S specific header rather than MSL
 */