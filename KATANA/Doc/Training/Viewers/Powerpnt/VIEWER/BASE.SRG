[HKEY_CLASSES_ROOT\CLSID\{FCE18141-B12B-11d0-B06A-00AA0060271A}\LocalServer]
@= "<product path short>\\<app name> /AUTOMATION"

[HKEY_CLASSES_ROOT\CLSID\{FCE18141-B12B-11d0-B06A-00AA0060271A}\LocalServer32]
@= "<product path>\\<app name> /AUTOMATION"

[HKEY_CLASSES_ROOT\CLSID\{FCE18141-B12B-11d0-B06A-00AA0060271A}\ProgID]
