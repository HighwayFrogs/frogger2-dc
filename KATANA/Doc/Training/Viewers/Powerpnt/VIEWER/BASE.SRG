#===================================================
#************ ENTRIES for Win95 StartUp Menu  
#===================================================
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Windows\CurrentVersion\App Paths\PPVIEW32.EXE]
@="<product path>\\<app name>"
"Path"= "<product path>"


#===================================================
#************ ENTRIES for Real Audio
#===================================================
[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Office\8.0\PowerPoint\DLL Addins\rappt]
"Path"="<product path>\\rappt.dll"


========================================================================
************ ENTRIES to register .lst presentations ************
========================================================================
[HKEY_CLASSES_ROOT\.lst]


[HKEY_CLASSES_ROOT\.lst\shell]
@="open"

[HKEY_CLASSES_ROOT\.lst\shell\Edit]
[HKEY_CLASSES_ROOT\.lst\shell\Edit\command]
@="Notepad \"%1\""

[HKEY_CLASSES_ROOT\.lst\shell\Open]
@="S&how"
[HKEY_CLASSES_ROOT\.lst\shell\Open\command]
@="<product path short>\\<app name> \"%1\""



#========================================================================
#************ ENTRIES to register Viewer for NetScape	    ************
# Install blindly since it won't do any harm
# [HKEY_CURRENT_USER\Software\Netscape\Netscape Navigator].
#========================================================================
[HKEY_CURRENT_USER\Software\Netscape\Netscape Navigator\Viewers]
"application/x-mspowerpoint"="<product path>\\<app name>"
"application/x-msppt"="<product path>\\<app name>"
"application/x-powerpoint"="<product path>\\<app name>"
"application/x-ppt"="<product path>\\<app name>"
"application/powerpoint"="<product path>\\<app name>"
"application/mspowerpoint"="<product path>\\<app name>"
"application/ms-powerpoint"="<product path>\\<app name>"


#===================================================
#************ ENTRIES for Viewer Object Model
#===================================================
[HKEY_CLASSES_ROOT\PowerPointViewer.Application]
@= "MS PowerPoint Viewer"

[HKEY_CLASSES_ROOT\PowerPointViewer.Application\CLSID]
@= "{FCE18141-B12B-11d0-B06A-00AA0060271A}"

[HKEY_CLASSES_ROOT\PowerPointViewer.Application\CurVer]
@= "PowerPointViewer.Application.8"

[HKEY_CLASSES_ROOT\PowerPointViewer.Application.8]
[HKEY_CLASSES_ROOT\PowerPointViewer.Application.8\CLSID]
@= "{FCE18141-B12B-11d0-B06A-00AA0060271A}"

[HKEY_CLASSES_ROOT\CLSID\{FCE18141-B12B-11d0-B06A-00AA0060271A}\InprocHandler]
@= "ole32.dll"

[HKEY_CLASSES_ROOT\CLSID\{FCE18141-B12B-11d0-B06A-00AA0060271A}\InprocHandler32]
